# Google Gemini API Key
# Get your API key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_api_key_here

# Server Port (optional, defaults to 3000)
PORT=3000

# Twitch Stream Key
# Get your stream key from: https://dashboard.twitch.tv/settings/stream
# Go to Twitch Dashboard -> Settings -> Stream -> Copy "Primary Stream Key"
TWITCH_STREAM_KEY=your_stream_key_here

# Optional: Twitch RTMP Server (defaults to rtmp://live.twitch.tv/app)
# You can specify a specific ingest server from: https://stream.twitch.tv/ingests/
# TWITCH_SERVER=rtmp://live.twitch.tv/app

# Test Mode - Set to 'true' to disable Twitch streaming (for development)
# When enabled, runs only the web server without streaming
# Set to 'false' or remove for production streaming
TEST_MODE=true
